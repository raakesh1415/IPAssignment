<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <display-name>Archetype Created Web Application</display-name>
    
    <absolute-ordering/>
    
    <servlet>
        <servlet-name>myFrontController</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>ProgramController</display-name>
    	<servlet-name>ProgramController</servlet-name>
    	<servlet-class>com.example.controller.ProgramController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>myFrontController</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>ProgramController</servlet-name>
    	<url-pattern>/ProgramController</url-pattern>
    </servlet-mapping>
</web-app>